<h1>Editing requirement_level</h1>

<% form_for :requirement_level, @requirement_level, :url => admin_requirement_level_path(@requirement_level) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', admin_requirement_level_path(@requirement_level) %> |
<%= link_to 'Back', admin_requirement_levels_path %>